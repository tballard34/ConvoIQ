{
  "name": "convoiq",
  "version": "1.0.0",
  "description": "ConvoIQ - AI-powered conversation analysis tool",
  "type": "module",
  "license": "Apache-2.0",
  "scripts": {
    "start": "harperdb run .",
    "dev": "npm run build && harperdb dev .",
    "build": "npm run build:frontend && npm run build:backend",
    "build:frontend": "vite build",
    "build:backend": "tsc resources.ts --outDir . --module ESNext --target ES2022 --moduleResolution node --skipLibCheck",
    "preview": "vite preview",
    "seed": "tsx scripts/seed.ts",
    "seed:components": "tsx scripts/seed-components.ts",
    "login": "bash -c 'read -r -p \"Cluster URL: \" URL; read -r -p \"Cluster Username: \" USERNAME; read -r -s -p \"Cluster Password: \" PASSWORD; echo; printf \"CLI_TARGET='%s'\\nCLI_TARGET_USERNAME='%s'\\nCLI_TARGET_PASSWORD='%s'\\n\" \"$URL\" \"$USERNAME\" \"$PASSWORD\" > .env'",
    "deploy": "npx -y dotenv-cli -- harperdb deploy . restart=rolling replicated=true",
    "generate-tree": "node scripts/generate-tree-rule.js"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.922.0",
    "@aws-sdk/s3-request-presigner": "^3.922.0",
    "@deepgram/sdk": "^4.11.2",
    "@tailwindcss/vite": "^4.1.16",
    "node-fetch": "^3.3.2",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-dropzone": "^14.3.8",
    "react-router-dom": "^7.9.5",
    "tailwindcss": "^4.1.16"
  },
  "devDependencies": {
    "@types/node": "^24.10.0",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@types/react-router-dom": "^5.3.3",
    "@vitejs/plugin-react": "^5.1.0",
    "tsx": "^4.20.6",
    "typescript": "^5.6.3",
    "vite": "^7.1.12"
  }
}
